            android:label="@string/app_name" />
        <activity
            android:name=".animationactivities.AccelerateRocketAnimationActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".animationactivities.LaunchRocketObjectAnimatorAnimationActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".animationactivities.ColorAnimationActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".animationactivities.LaunchAndSpinAnimatorSetAnimatorActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".animationactivities.LaunchAndSpinViewPropertyAnimatorAnimationActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".animationactivities.FlyWithDogeAnimationActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".animationactivities.WithListenerAnimationActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".animationactivities.FlyThereAndBackAnimationActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".animationactivities.XmlAnimationActivity"
            android:label="@string/app_name" />
    </application>

</manifest>