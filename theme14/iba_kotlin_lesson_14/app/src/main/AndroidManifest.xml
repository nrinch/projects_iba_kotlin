        android:theme="@style/Theme.Iba_kotlin_lesson_14">
        <activity android:name=".FragmentActivity"></activity>
        <activity android:name=".GestureActivity" />
        <activity android:name=".DrawableAnimActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".animationactivities.NoAnimationActivity"
            android:label="@string/app_name" />
        <activity
